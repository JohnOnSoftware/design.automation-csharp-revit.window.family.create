		{
			"name": "04-Create activity",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "a1eda547-e6c7-4073-a773-0fc897166bfa",
						"exec": [
							"var body = JSON.parse(responseBody);",
							"pm.environment.set(\"activityVersion\", body[\"version\"]);",
							""
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "Content-Type",
						"value": "application/json"
					},
					{
						"key": "Authorization",
						"value": "Bearer {{dasApiToken}}"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\r\n\t\"id\": \"{{activityName}}\",\r\n\t\"commandLine\": [ \"$(engine.path)\\\\\\\\revitcoreconsole.exe /i $(args[rvtFile].path) /al $(appbundles[{{appBundleName}}].path)\" ],\r\n\t\"parameters\": {\r\n\t  \"rvtFile\": {\r\n\t\t\"zip\": false,\r\n\t\t\"ondemand\": false,\r\n\t\t\"verb\": \"get\",\r\n\t\t\"description\": \"Input Revit model\",\r\n\t\t\"required\": true\r\n\t  },\r\n\t  \"resultrvt\": {\r\n\t\t\"zip\": false,\r\n\t\t\"ondemand\": false,\r\n\t\t\"verb\": \"put\",\r\n\t\t\"description\": \"upgraded revit Rvt file\",\r\n\t\t\"required\": false,\r\n\t\t\"localName\": \"revitupgrade.rvt\"\r\n\t  },\r\n\t  \"resultrfa\": {\r\n\t\t\"zip\": false,\r\n\t\t\"ondemand\": false,\r\n\t\t\"verb\": \"put\",\r\n\t\t\"description\": \"upgraded revit Rfa file\",\r\n\t\t\"required\": false,\r\n\t\t\"localName\": \"revitupgrade.rfa\"\r\n\t  },\r\n\t  \"resultrte\": {\r\n\t\t\"zip\": false,\r\n\t\t\"ondemand\": false,\r\n\t\t\"verb\": \"put\",\r\n\t\t\"description\": \"upgraded revit Rte file\",\r\n\t\t\"required\": false,\r\n\t\t\"localName\": \"revitupgrade.rte\"\r\n\t  }\r\n\t},\r\n\t\"engine\": \"Autodesk.Revit+2019\",\r\n\t\"appbundles\": [ \"{{dasNickName}}.{{appBundleName}}+{{appBundleAlias}}\" ],\r\n\t\"description\": \"Upgrade revit file to lastest version from old versions.\"\r\n}"
				},
				"url": {
					"raw": "{{dasApiRoot}}/activities",
					"host": [
						"{{dasApiRoot}}"
					],
					"path": [
						"activities"
					]
				}
